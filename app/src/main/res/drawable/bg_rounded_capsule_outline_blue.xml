<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#F2F6FB"/>
            <corners android:radius="16dp" />
            <stroke android:color="@color/primaryBlue"
                android:width="1dp" />
        </shape>
    </item>

    <item
        android:left="2dp"
        android:right="2dp"
        android:top="2dp"
        android:bottom="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#F2F6FB"/>
            <corners android:radius="16dp" />
        </shape>
    </item>
</layer-list>